{
  "name": "copilot-agent-console",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/dev.js",
    "setup": "pip install -e . && npm install --prefix frontend",
    "dev:backend": "python -m uvicorn copilot_agent_console.app.main:app --reload --port 8765",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
